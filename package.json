{
  "name": "immutable-data",
  "version": "2.0.5",
  "description": "Easily update nested objects and arrays in a declarative and immutable manner",
  "main": "lib/index.js",
  "entry": {
    "index": "./src/index.js"
  },
  "scripts": {
    "test-chrome": "atool-test --browsers Chrome",
    "test": "atool-test",
    "build": "atool-build -o ./lib --no-compress",
    "coveralls": "cat ./coverage/report-lcov/lcov.info | coveralls",
    "prepublish": "npm run build",
    "dev": "dora -p 8001 --plugins dora-plugin-atool-build?publicPath=/demo"
  },
  "author": "https://github.com/zinkey",
  "homepage": "https://github.com/flutejs/immutable-data",
  "repository": {
    "type": "git",
    "url": "https://github.com/flutejs/immutable-data"
  },
  "bugs": {
    "url": "https://github.com/flutejs/immutable-data/issues"
  },
  "licenses": "MIT",
  "dependencies": {
    "array-foreach": "^1.0.1",
    "array-map": "0.0.0",
    "is-array": "~1.0.1",
    "is-plain-object": "^2.0.1",
    "object-assign": "~4.0.1",
    "object-keys": "^1.0.9",
    "object-path-parse": "^0.1.0"
  },
  "devDependencies": {
    "atool-build": "0.5.x",
    "atool-test": "*",
    "coveralls": "~2.11.4",
    "dora": "0.3.0",
    "dora-plugin-atool-build": "0.4.x",
    "karma-chrome-launcher": "^0.2.2"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "immutable",
    "persistent",
    "data",
    "datastructure",
    "declarative",
    "nested"
  ]
}
